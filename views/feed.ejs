<%- include('partials/header2') -%>
  <div class="wrapper">
    <div class="container mt-5">
      <h3 class="text-center">Social Feed</h3>
      <ul class="row justify-content-center mt-5">
        <% for(var i=0; i<posts.length; i++) {%>
          <div class="card mt-3 col-lg-7" style="background-color: #f5f0f0;">
            <div class="card-img-top text-center pt-3">
              <li class="">
                <a href="/post/<%= posts[i]._id%>">
                  <img class="resize-img" src="<%= posts[i].image%>">
                </a>
              </li>
            </div>
            <div class="card-body">
              <h5 class="card-title">
                <%= posts[i].title%>
              </h5>
              <p>
                posted by <%= posts[i].userName%>
              </p>
              <p class="card-text">
                Likes: <%= posts[i].likes%> <br />
                  <%= posts[i].caption%>
              </p>
            </div>
          </div>
          <% } %>
      </ul>
    </div>
  </div>
  <%- include('partials/footer') -%>